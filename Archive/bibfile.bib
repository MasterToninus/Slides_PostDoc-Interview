@article{Barnich1998,
abstract = {A general construction of an sh Lie algebra (L ∞-algebra) from a homological resolution of a Lie algebra is given. It is applied to the space of local functionals equipped with a Poisson bracket, induced by a bracket for local functions along the lines suggested by Gel'fand, Dickey and Dorfman. In this way, higher order maps are constructed which combine to form an sh Lie algebra on the graded differential algebra of horizontal forms. The same construction applies for graded brackets in field theory such as the Batalin-Fradkin-Vilkovisky bracket of the Hamiltonian BRST theory or the Batalin-Vilkovisky antibracket.},
archivePrefix = {arXiv},
arxivId = {hep-th/9702176},
author = {Barnich, Glenn and Fulp, R. and Lada, Tom and Stasheff, Jim},
doi = {10.1007/s002200050278},
eprint = {9702176},
file = {:home/tony/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Barnich et al. - 1998 - The sh Lie Structure of Poisson Brackets in Field Theory.pdf:pdf;:home/tony/Scaricati/Barnich1998{\_}Article{\_}TheShLieStructureOfPoissonBrac.pdf:pdf},
issn = {0010-3616},
journal = {Communications in Mathematical Physics},
keywords = {Physics and Astronomy},
month = {feb},
number = {3},
pages = {585--601},
primaryClass = {hep-th},
title = {{The sh Lie Structure of Poisson Brackets in Field Theory}},
url = {http://www.springerlink.com/content/ut90k4haydhem17n/ http://link.springer.com/10.1007/s002200050278 https://arxiv.org/pdf/hep-th/9702176.pdf},
volume = {191},
year = {1998}
}
@article{Miti2018,
abstract = {In this note we construct a homotopy co-momentum map ([8, 56, 57]) trangressing to the standard hydrodynamical co-momentum map of Arnol'd, Marsden and Weinstein and others, then generalized to a special class of Riemannian manifolds. As a byproduct, a covariant phase space interpretation of Brylinski's manifold of mildly singular links is exhibited upon resorting to the Euler equation for perfect fluids. A semiclassical interpretation of the HOMFLYPT polynomial is also given, building on the Liu-Ricca hydrodynamical approach to the latter and on the Besana- S. symplectic approach to framing. We finally reinterpret the (Massey) higher order linking numbers in terms of conserved quantities within the multisymplectic framework and determine knot theoretic analogues of first integrals in involution.},
archivePrefix = {arXiv},
arxivId = {1805.01696},
author = {Miti, Antonio Michele and Spera, Mauro},
eprint = {1805.01696},
month = {may},
title = {{On some (multi)symplectic aspects of link invariants}},
url = {http://arxiv.org/abs/1805.01696},
journal = {arXiv:1805.01696},
year = {2018}
}

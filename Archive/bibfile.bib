@article{Rogers2010,
abstract = {A manifold is multisymplectic, or more specifically n-plectic, if it is equipped with a closed nondegenerate differential form of degree n+1. In our previous work with Baez and Hoffnung, we described how the `higher analogs' of the algebraic and geometric structures found in symplectic geometry should naturally arise in 2-plectic geometry. In particular, we showed that just as a symplectic manifold gives a Poisson algebra of functions, any 2-plectic manifold gives a Lie 2-algebra of 1-forms and functions. Lie n-algebras are examples of L-infinity algebras: graded vector spaces which are equipped with a collection of skew-symmetric multi-brackets that satisfy a generalized Jacobi identity. Here, we generalize our previous result. Given an n-plectic manifold, we explicitly construct a corresponding Lie n-algebra on a complex consisting of differential forms whose multi-brackets are specified by the n-plectic structure. We also show that any n-plectic manifold gives rise to another kind of algebraic structure known as a differential graded Leibniz algebra. We conclude by describing the similarities between these two structures within the context of an open problem in the theory of strongly homotopy algebras.},
archivePrefix = {arXiv},
arxivId = {1005.2230},
author = {Rogers, Christopher L.},
doi = {10.1007/s11005-011-0493-x},
eprint = {1005.2230},
issn = {0377-9017},
journal = {Letters in Mathematical Physics},
keywords = {()},
mendeley-groups = {Dottorato/Estate2018},
month = {apr},
number = {1},
pages = {29--50},
title = {{L $\infty$-Algebras from Multisymplectic Geometry}},
url = {https://arxiv.org/pdf/1005.2230.pdf http://arxiv.org/abs/1005.2230 http://dx.doi.org/10.1007/s11005-011-0493-x http://link.springer.com/10.1007/s11005-011-0493-x http://arxiv.org/abs/1005.2230{\%}0Ahttp://dx.doi.org/10.1007/s11005-011-0493-x},
volume = {100},
year = {2012}
}

@article{Callies2016,
abstract = {Associated to any manifold equipped with a closed form of degree {\textgreater}1 is an ‘L$\infty$-algebra of observables' which acts as a higher/homotopy analog of the Poisson algebra of functions on a symplectic manifold. In order to study Lie group actions on these manifolds, we introduce a theory of homotopy moment maps. Such a map is a L$\infty$-morphism from the Lie algebra of the group into the observables which lifts the infinitesimal action. We establish the relationship between homotopy moment maps and equivariant de Rham cohomology, and analyze the obstruction theory for the existence of such maps. This allows us to easily and explicitly construct a large number of examples. These include results concerning group actions on loop spaces and moduli spaces of flat connections. Relationships are also established with previous work by others in classical field theory, algebroid theory, and dg geometry. Furthermore, we use our theory to geometrically construct various L$\infty$-algebras as higher central extensions of Lie algebras, in analogy with Kostant's quantization theory. In particular, the so-called ‘string Lie 2-algebra' arises this way.},
archivePrefix = {arXiv},
arxivId = {1304.2051},
author = {Callies, Martin and Fr{\'{e}}gier, Ya{\"{e}}l and Rogers, Christopher L. and Zambon, Marco},
doi = {10.1016/j.aim.2016.08.012},
eprint = {1304.2051},
issn = {00018708},
journal = {Advances in Mathematics},
keywords = {Equivariant cohomology,Moment map,Multisymplectic geometry,Strong homotopy Lie algebra},
mendeley-groups = {Dottorato/Estate2018},
month = {nov},
pages = {954--1043},
title = {{Homotopy moment maps}},
url = {http://arxiv.org/abs/1304.2051 https://linkinghub.elsevier.com/retrieve/pii/S0001870816310192},
volume = {303},
year = {2016}
}



@article{Ryvkin2016,
abstract = {Given a vector field on a manifold {\$}M{\$} , we define a globally conserved quantity to be a differential form whose Lie derivative is exact. Integrals of conserved quantities over suitable submanifolds are constant under time evolution, the Kelvin circulation theorem being a well-known special case. More generally, conserved quantities are well behaved under transgression to spaces of maps into {\$}M{\$} . We focus on the case of multisymplectic manifolds and Hamiltonian vector fields. Our main result is that in the presence of a Lie group of symmetries admitting a homotopy co-momentum map, one obtains a whole family of globally conserved quantities. This extends a classical result in symplectic geometry. We carry this out in a general setting, considering several variants of the notion of globally conserved quantity.},
author = {Ryvkin, Leonid and Wurzbacher, Tilmann and Zambon, Marco},
doi = {10.1017/S1446788718000381},
issn = {1446-7887},
journal = {J. Aust. Math. Soc.},
keywords = {conservation laws,homotopy co-momentum map,multisymplectic manifolds},
month = {dec},
pages = {1--25},
title = {{Conserved quantities on multisymplectic manifolds}},
url = {https://doi.org/10.1017/S1446788718000381 https://www.cambridge.org/core/product/identifier/S1446788718000381/type/journal{\_}article},
year = {2018}
}



@article{Ryvkin2015,
abstract = {Given a multisymplectic manifold (M, ??) and a Lie algebra g acting on it by infinitesimal symmetries, Fregier-Rogers-Zambon define a homotopy (co-)moment as an L{\textless}inf{\textgreater}???{\textless}/inf{\textgreater}-algebra-homomorphism from g to the observable algebra L(M, ??) associated to (M, ??), in analogy with and generalizing the notion of a co-moment map in symplectic geometry. We give a cohomological characterization of existence and unicity for homotopy co-moment maps and show its utility in multisymplectic geometry by applying it to special cases as exact multisymplectic manifolds and simple Lie groups and by deriving from it existence results concerning partial co-moment maps, as e.g. covariant multimomentum maps and multi-moment maps.},
